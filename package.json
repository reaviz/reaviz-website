{
  "name": "reaviz-website",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.1.2",
  "scripts": {
    "dev": "next dev",
    "copy:stories": "cpx \"node_modules/reaviz/dist/stories/*.tsx\" \"src/stories/components\" -C",
    "copy:blocks": "cpx \"node_modules/reaviz/dist/blocks/*.tsx\" \"src/stories/blocks\" -C",
    "build": "next build",
    "postinstall": "pnpm run copy:stories && pnpm run copy:blocks",
    "postbuild": "pagefind --site .next/server/app --output-path public/_pagefind",
    "start": "next start",
    "lint": "next lint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "dependencies": {
    "@mdx-js/mdx": "^3.0.1",
    "@mdx-js/react": "^3.0.1",
    "chroma-js": "^3.1.2",
    "clsx": "^2.1.0",
    "d3-array": "^3.2.4",
    "d3-shape": "^3.2.0",
    "d3-time": "^3.1.0",
    "date-fns": "^3.6.0",
    "motion": "^11.18.0",
    "next": "^15.4.1",
    "nextra": "^4.2.17",
    "posthog-js": "^1.131.4",
    "reablocks": "^8.7.4",
    "reablocks-docs-theme": "^2.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-live-runner": "^1.0.7",
    "react-runner": "^1.0.5",
    "react-syntax-highlighter": "^15.5.0",
    "reaviz": "^16.0.5",
    "reaviz-data-utils": "^1.0.0",
    "sharp": "^0.33.3",
    "tailwind-merge": "^2.2.2",
    "topojson-client": "^3.1.0",
    "world-atlas": "^2.0.2"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4.1.10",
    "@types/chroma-js": "^3.1.1",
    "@types/d3-array": "^3.2.1",
    "@types/d3-shape": "^3.1.7",
    "@types/d3-time": "^3.0.4",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/topojson-client": "^3.1.5",
    "cpx": "^1.5.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.1",
    "pagefind": "^1.3.0",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "storybook": "7.6.3",
    "tailwindcss": "^4.1.11",
    "typescript": "^5"
  }
}
